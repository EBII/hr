<odoo>
    <!-- Employment Group Coef -->
    <record id="action_hr_employment_group_level_tree"
            model="ir.actions.act_window">
        <field name="name">Employment Group Level</field>
        <field name="res_model">hr.employment.group.level</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form,graph</field>
    </record>

    <menuitem id="menu_action_employment_group_level"
              action="action_hr_employment_group_level_tree"
              parent="hr.menu_human_resources_configuration"/>


    <record id="hr_employment_group_coef_view_tree" model="ir.ui.view">
        <field name="name">hr.employment.group.coef.view.tree</field>
        <field name="model">hr.employment.group.coef</field>
        <field name="arch" type="xml">
            <tree name="Employment Group Coef">
                <field name="name"/>
                <field name="collective_agreement_id"/>
            </tree>
        </field>
    </record>

    <record id="hr_employment_group_coef_view_form" model="ir.ui.view">
        <field name="name">hr.employment.group.coef.view.form</field>
        <field name="model">hr.employment.group.coef</field>
        <field name="arch" type="xml">
            <form name="Employment Group Coef">
                <group>
                    <field name="name"/>
                    <field name="collective_agreement_id"/>
                </group>
            </form>
        </field>
    </record>
    <record id="employment_group_coef_search" model="ir.ui.view">
        <field name="model">hr.employment.group.coef</field>
        <field name="arch" type="xml">
            <search string="">
                <field name="collective_agreement_id" string="Collective Agreement" domain="[()]"/>
            </search>
        </field>
    </record>
    <record id="action_hr_employment_group_coef_tree"
            model="ir.actions.act_window">
        <field name="name">Employment Group Coef</field>
        <field name="res_model">hr.employment.group.coef</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form,graph</field>
    </record>

    <menuitem id="menu_action_employment_group_coef"
              action="action_hr_employment_group_coef_tree"
              parent="hr.menu_human_resources_configuration"/>


    <menuitem action="hr.action_hr_job" id="menu_job_title_hr"
              parent="hr.menu_human_resources_configuration"/>

    <!-- Employment Group Level -->
    <record id="hr_employment_group_level_view_tree" model="ir.ui.view">
        <field name="name">hr.employment.group.level.view.tree</field>
        <field name="model">hr.employment.group.level</field>
        <field name="arch" type="xml">
            <tree name="Employment Group Level">
                <field name="name"/>
                <field name="collective_agreement_id" options="{'no_create': True}"/>
            </tree>
        </field>
    </record>
    <record id="employment_group_level_search" model="ir.ui.view">
        <field name="model">hr.employment.group.level</field>
        <field name="arch" type="xml">
            <search string="">
                <field name="collective_agreement_id" string="Collective Agreement" domain="[()]"/>
            </search>
        </field>
    </record>
    <record id="hr_employment_group_level_view_form" model="ir.ui.view">
        <field name="name">hr.employment.group.level.view.form</field>
        <field name="model">hr.employment.group.level</field>
        <field name="arch" type="xml">
            <form name="Employment Group Level">
                <group>
                    <field name="name"/>
                    <field name="collective_agreement_id" options="{'no_create': True}"/>
                </group>
            </form>
        </field>
    </record>

</odoo>